<section class="posts">
    <h3>Posts</h3>
    
    {% for post in collections.posts %}
    <div class="post">
        <p>
            <a href="{{ post.url }}" title="View {{ post.data.title }}" tabindex="1">{{ post.data.title }}</a>
            <time datetime="{{ post.date | postDate }}">{{ post.date | postDate }}</time>
        </p>
    </div>
    {% endfor %}
    
</section>

